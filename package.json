{
    "name": "postgresql-exploring-backend",
    "version": "1.0.0",
    "description": "Exploring the capabilities of postgresql in comparison with other DBMS ",
    "main": "src/app.js",
    "scripts": {
        "setup": "npm install && npm update",
        "start": "cross-env NODE_ENV=local nodemon src/app.js",
        "migration:undo": "cross-env MIG_ENV=local npx sequelize-cli db:migrate:undo",
        "migration:add": "npx sequelize-cli migration:create --name",
        "migrate": "npx sequelize-cli db:migrate"
    },
    "keywords": [
        "express",
        "backend"
    ],
    "license": "UNLICENSED",
    "dependencies": {
        "cls-hooked": "^4.2.2",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "express": "^4.17.1",
        "express-custom-error": "^1.8.4",
        "has-keys": "^0.1.0",
        "http-status": "^1.3.2",
        "mariadb": "^3.0.0",
        "module-alias": "^2.2.2",
        "mysql2": "^2.3.3",
        "nodemon": "^2.0.11",
        "pg": "^8.7.3",
        "sequelize": "6.3.5",
        "sequelize-cli": "^6.2.0",
        "tedious": "^14.6.0",
        "winston": "^3.8.0",
        "winston-daily-rotate-file": "^4.7.1"
    }
}
